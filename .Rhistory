saveRDS(data_sen1, "data_sen1_sparse.rds")
sen1_Y_mat <- data_sen1$Y_mat
sen1_A <- data_sen1$A
sen1_X <- data_sen1$X
sen1_Y_list <- data_sen1$Y_list
# Do not run it again.
#Fit the MOM-SS model
set.seed(10)
result_MOMSS_sen1 <- BFR.BE.EM.CV(x = sen1_Y_mat, v = sen1_X,
