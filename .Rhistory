#Fit the BMSFA model
result_BMSFA_sen1 <- sp_msfa(sen1_Y_list, k = 5, j_s = c(1,1,1,1), scaling = FALSE,
control = list(nrun = 2000, burn = 1000),
outputlevel = 3)
saveRDS(data_sen1, "data_sen1_sparse.rds")
saveRDS(data_sen1, "../RDS/data_sen1_sparse.rds")
